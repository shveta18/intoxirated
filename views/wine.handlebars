<div>

    <h2>Wines</h2>

    <ul>
        {{#each wines}}  
        <p>{{this.dataValues.id}}. Name: {{this.dataValues.name}}, Manufacturer: {{this.dataValues.manufacturer}}, Style: {{this.dataValues.style}}, Year: {{this.dataValues.year}}, Rating: {{this.dataValues.rating}}</p>
        {{/each}}
    </ul>

</div>